    if R10 == 2 
        R10 = 'O';
    end
end
if OgrNoVektorel(10) == 7 
    R10 = randi(3);
    if R10 == 1 
        R10 = 'G';
    end
    if R10 == 2 
        R10 = 'P';
    end
    if R10 == 3 
        R10 = 'Y';
    end
end
if OgrNoVektorel(10) == 8 
    R10 = randi(2);
    if R10 == 1 
        R10 = 'H';
    end
    if R10 == 2 
        R10 = 'Z';
    end
end
if OgrNoVektorel(10) == 9 
    R10 = randi(2);
    if R10 == 1 
        R10 = 'I';
    end
    if R10 == 2 
        R10 = 'R';
    end
end
fprintf('%c%c%c%c%c%c%c%c%c%c \n' ,R1,R2,R3,R4,R5,R6,R7,R8,R9,R10) 
end
else
    disp('Verilen Sayı Öğrenci Numarası Formatında Değildir.')
end
